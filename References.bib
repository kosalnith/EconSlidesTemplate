@article{SahulHamid2020,
abstract = {We analyze the lending cyclicality of 213 ASEAN commercial banks over the period 2001–2015. The findings indicate that lending by private banks is procyclical while lending by state banks is countercyclical. Long-term liabilities also move countercyclically for state banks whereas funding for non-state banks in the form of deposit and long-term liabilities is procyclical. Greater lending cyclicality is observed for both private and state banks in Cambodia, Myanmar, Laos, and Vietnam (CMLV) compared to Indonesia, Malaysia, the Philippines, Thailand, and Singapore (ASEAN-5). Lending of non-ASEAN based foreign banks shows greater procyclicality than that of domestic banks for the ASEAN-5 countries, although not for the CMLV countries. During the global financial crisis, lending by non-ASEAN based foreign banks contracted sharply even as lending by ASEAN based foreign banks was unaffected. Overall, our results confirm that bank ownership influences lending and funding sensitivity to economic fluctuations.},
author = {{Sahul Hamid}, Fazelina},
doi = {10.1016/j.asieco.2019.101153},
issn = {10490078},
journal = {Journal of Asian Economics},
keywords = {Bank Lending,Bank lending,Bank ownership,Business cycle and ASEAN banking,Government Owned-Bank},
mendeley-tags = {Bank Lending,Government Owned-Bank},
month = {feb},
pages = {101153},
publisher = {Elsevier Inc.},
title = {{Bank lending and the business cycle: Does ownership matter in ASEAN countries?}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S104900781830318X},
volume = {66},
year = {2020}
}
