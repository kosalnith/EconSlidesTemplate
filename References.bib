@article{Al-Hyari2013,
abstract = {The purpose of this paper is to investigate the factors that hinder the growth and survival of SMEs in Jordan. The methodology adopted in this research is critical literature reviews, quantitative data collection through questionnaires to 241 SMEs 'owners and managers located in selected cities in Jordan. The results of the study reveal that the most common constraints hindering SMEs growth and survival in Jordan are: lack of financial support; competition barriers; lack of qualified human resources; and rigid business rules and regulations. Most SMEs agreed that government should develop program that is strongly required as a strategic move to emerge the economy and dedicate more efforts to eliminate SMEs barriers, like encourage the financial institutes and banks to provide suitable credit facilities to this sector. Legal infrastructure such as privacy law, e-signature and knowledge acquisition law, facilitate SME with technology through soft loan to buy supporting technology for their business, and many supporting program needed by SMEs. The significance of this study stems from the fact that very few studies have explored the issue of factors constraining the growth and survival of SMEs in Jordan. Also, it offers guidance for future research in a field of research that is still in its infancy.},
author = {Al-Hyari, Khalil},
doi = {10.5539/ijbm.v8n24p132},
file = {:C$\backslash$:/Users/Nith Kosal/Documents/Kosal Documents/Research Projects/2020/2020{\_}01/Working Papers/10.1.1.966.9938.pdf:pdf},
